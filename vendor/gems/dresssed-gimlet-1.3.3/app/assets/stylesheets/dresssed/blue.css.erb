  -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.6);
  -moz-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.6);
  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.6);
  margin-left: -14px;
  margin-right: -14px;
  margin-bottom: 0;
  padding: 10px 14px;
}
.form-well .control-group:first-child {
  margin-top: -14px;
  border-top: none;
